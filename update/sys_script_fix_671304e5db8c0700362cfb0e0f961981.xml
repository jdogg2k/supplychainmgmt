<record_update table="sys_script_fix">
    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description/>
        <flush_cache>false</flush_cache>
        <name>Supplier Geocode</name>
        <run_once>false</run_once>
        <script><![CDATA[var gr = new GlideRecord("x_kfna2_supply_cha_suppliers");
gr.addEncodedQuery("country=USA^state!=NULL^city!=NULL^state!=Canada^state!=MEX");
gr.query();
while (gr.next()) {
var tAddress = gr.getValue("city") + ', ' + gr.getValue("state");
	
   var point=GoogleMapsHelper.google_getLatLong(tAddress + '');
   if (point[0] && point[1]) {
      gr.lat_coord = point[0];
      gr.lon_coord = point[1];
	  gr.update();
   }
}]]></script>
